{
  "name": "memory-markets",
  "version": "0.1.0",
  "description": "Where Agents Trade Intelligence - A marketplace for AI agent knowledge on Monad",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "mm": "bin/mm.js"
  },
  "scripts": {
    "mm": "tsx src/cli/index.ts",
    "dev": "tsx watch src/cli/index.ts",
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "verify": "tsx scripts/verify-env.ts",
    "lint": "tsc --noEmit",
    "demo": "tsx scripts/demo-full.ts",
    "demo:quick": "tsx scripts/demo-full.ts --skip-export --skip-blockchain",
    "demo:export": "tsx examples/demo-export-import.ts",
    "demo:blockchain": "tsx examples/demo-blockchain.ts",
    "demo:agent": "tsx examples/demo-agent.ts"
  },
  "keywords": [
    "ai-agents",
    "knowledge-marketplace",
    "monad",
    "nadfun",
    "blockchain",
    "context-transfer",
    "rag",
    "embeddings"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "@hono/node-server": "^1.0.0",
    "@modelcontextprotocol/sdk": "^1.26.0",
    "@x402/core": "^2.3.1",
    "@x402/evm": "^2.3.1",
    "@x402/fetch": "^2.3.0",
    "@x402/hono": "^2.3.0",
    "chalk": "^5.0.0",
    "cli-table3": "^0.6.0",
    "commander": "^12.0.0",
    "dotenv": "^16.0.0",
    "hono": "^4.0.0",
    "openai": "^6.18.0",
    "ora": "^8.0.0",
    "sql.js": "^1.11.0",
    "viem": "^2.0.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-viem": "^2.1.3",
    "@openzeppelin/contracts": "^5.4.0",
    "@types/node": "^22.0.0",
    "@types/sql.js": "^1.4.9",
    "hardhat": "^2.28.6",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.7.0",
    "vitest": "^2.0.0"
  }
}
